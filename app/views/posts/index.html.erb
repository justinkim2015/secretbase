<% stylesheet_link_tag "index" %>

<% @posts.each do |post| %>
  <div class="post">
    <div><%= post.title %> - <%= post.body %></div>
    <div><%= post.user.username %></div>
  </div>
<% end  %>

<%= link_to "New Post", new_post_path, class: "link" %>
<%= link_to "User Info", "/users/edit", class: "link" %>

